{{#each articles}}
     <div class="card">
          <div class="card-header">
               <h3><a href={{link}}>{{title}}</a></h3>
          </div>
          <div class="card-body">
               <p class="card-text">{{summary}}</p>
               <a href="#" id="{{_id}}" class="btn btn-primary savebtn">Save Article</a>
          </div>
     </div>
{{/each}}